<p id="notice"><%= notice %></p>

<h1>Listing Envapps</h1>

<table>
  <thead>
    <tr>
      <th>Env</th>
      <th>App</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @envapps.each do |envapp| %>
      <tr>
        <td><%= envapp.env %></td>
        <td><%= envapp.app %></td>
        <td><%= link_to 'Show', envapp %></td>
        <td><%= link_to 'Edit', edit_envapp_path(envapp) %></td>
        <td><%= link_to 'Destroy', envapp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Envapp', new_envapp_path %>
